<h1>Users</h1>

 <table class="user-list">
   <thead>
     <th>image</th>
     <th>name</th>
     <th colspan="3"></th>
   </thead>
   <tbody>
     <% @users.each do |user| %>
     <tr>
       <td><%= image_tag user.get_profile_image(50,50) %></td>
       <td><%= user.name %></td>
       <td><%= link_to "show", user_path(user.id) %></td>
     </tr>
     <% end %>
   </tbody>

 </table>
